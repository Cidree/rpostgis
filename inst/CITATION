citHeader("To cite rpostgis in publications use:")

citEntry(entry = "Manual",
  title        = "rpostgis: R Interface to a 'PostGIS' Database",
  author       = personList(as.person("Mathieu Basille"),
                            as.person("David Bucklin")),
  year         = "2018",
  url          = "https://mablab.org/rpostgis",

  textVersion  =
  paste("Mathieu Basille and David Bucklin (2018). rpostgis: R Interface to a 'PostGIS' Database. https://mablab.org/rpostgis")
)
